        <activity android:name=".BookWishList" />
        <activity android:name=".UImain.AlreadyReadBooks" />
        <activity android:name=".BookAcitivity" />
        <activity android:name=".UImain.MyBooks" />
        <activity android:name=".UImain.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>