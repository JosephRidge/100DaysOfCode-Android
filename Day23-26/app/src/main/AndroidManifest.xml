<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jayr.soma">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_audio_book_book_music_player_headphone_learning"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_audio_book_book_music_player_headphone_learning"
        android:supportsRtl="true"
        android:theme="@style/Theme.Soma">
        <activity android:name=".ReadBook"></activity>
        <activity android:name=".BookActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>